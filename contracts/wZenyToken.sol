contract wZenyToken is Context, Ownable {
    /* --------- Math info --------- */

    using SafeMath for uint256;

    /* --------- exchange info --------- */
    IDMMRouter02 public DMMRouter02;
    address public DMMV2Pair;

    uint256 private _totalSupply;
    uint8 private _decimals;

    /* --------- max tx info --------- */
    uint256 public _sellLimit = 100000 * 1e18; // sell limit: 1,000,000,000,000,000,000,000,000

    string private _symbol;
    string private _name;

    /* --------- special address info --------- */
    address public burnAddress = 0x000000000000000000000000000000000000dEaD;
    address public taxWallet1 = 0xba48e82Fa5586DF0C35f45b18287490bCA0AE6f3;
    address public taxWallet2 = 0x7ECC20257C5F5cBCB3010053CafA12ff80CC81F7;

    /* --------- mapping define --------- */

    mapping(address => uint256) private _balances;
    mapping(address => mapping(address => uint256)) private _allowances;

    /* --------- struct define --------- */

    struct FeesStruct {
        uint256 buyFee;
        uint256 sellFee;
    }

    FeesStruct public _taxRates = FeesStruct({buyFee: 25, sellFee: 50});

    /* --------- event define --------- */

    event SetTaxRate(FeesStruct taxRates);
    event Transfer(address indexed from, address indexed to, uint256 value);
    event Approval(address indexed owner, address indexed spender, uint256 value);
    event SwapAndLiquify( uint256 tokensSwapped, uint256 ethReceived, uint256 tokensIntoLiqudity);


    ////////////////////////////////////////////////
    /* --------- General Implementation --------- */
    ////////////////////////////////////////////////

    function getOwner() external view returns (address) {
        return owner();
    }

    function decimals() external view returns (uint8) {
        return _decimals;
    }

    function symbol() external view returns (string memory) {
        return _symbol;
    }

    function name() external view returns (string memory) {
        return _name;
    }

    function totalSupply() external view returns (uint256) {
        return _totalSupply;
    }

    function balanceOf(address account) public view returns (uint256) {
        return _balances[account];
    }

    function transfer(address recipient, uint256 amount)
        external
        returns (bool)
    {
        _transfer(_msgSender(), recipient, amount);
        return true;
    }

    function allowance(address owner, address spender)
        external
        view
        returns (uint256)
    {
        return _allowances[owner][spender];
    }

    function approve(address spender, uint256 amount) external returns (bool) {
        _approve(_msgSender(), spender, amount);
        return true;
    }

    function transferFrom(
        address sender,
        address recipient,
        uint256 amount
    ) external returns (bool) {
        _transfer(sender, recipient, amount);
        _approve(
            sender,
            _msgSender(),
            _allowances[sender][_msgSender()].sub(
                amount,
                "wZeny::transferFrom: transfer amount exceeds allowance"
            )
        );
        return true;
    }

    function increaseAllowance(address spender, uint256 addedValue)
        external
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender].add(addedValue)
        );
        return true;
    }

    function decreaseAllowance(address spender, uint256 subtractedValue)
        external
        returns (bool)
    {
        _approve(
            _msgSender(),
            spender,
            _allowances[_msgSender()][spender].sub(
                subtractedValue,
                "wZeny::decreaseAllowance: decreased allowance below zero"
            )
        );
        return true;
    }

    function _approve(
        address owner,
        address spender,
        uint256 amount
    ) internal {
        require(
            owner != address(0),
            "wZeny::_approve approve from the zero address"
        );
        require(
            spender != address(0),
            "wZeny::_approve approve to the zero address"
        );

        _allowances[owner][spender] = amount;
        emit Approval(owner, spender, amount);
    }

    //////////////////////////////////////////////
    /* ----------- special features ----------- */
    //////////////////////////////////////////////

    constructor() public {
        _name = "World of Ragnarok Zenny";
        _symbol = "wZeny";
        _decimals = 18;
        _totalSupply = 10000000 * 1e18;
        _balances[msg.sender] = _totalSupply;

        IDMMRouter02 _DMMRouter02 = IDMMRouter02(0xD536e64EAe5FBc62E277167e758AfEA570279956);
        DMMRouter02 = _DMMRouter02;
        DMMV2Pair = IDMMFactory(_DMMRouter02.factory()).createPool(IERC20(address(this)), IERC20(_DMMRouter02.weth()), 100000000);

        emit Transfer(address(0), msg.sender, _totalSupply);
    }

    /* --------- set token parameters--------- */

    function setInitialAddresses(address _RouterAddress) external onlyOwner {
        IDMMRouter02 _DMMRouter02 = IDMMRouter02(
            _RouterAddress
        );
        DMMRouter02 = _DMMRouter02;
        DMMV2Pair = IDMMFactory(_DMMRouter02.factory()).createPool(IERC20(address(this)), IERC20(_DMMRouter02.weth()), 100000000);
    }

    function setSellLimitAmount(uint256 limitTxAmount) external onlyOwner {
        _sellLimit = limitTxAmount;
    }

    function setBuyFee(
        uint256 buyFee
    ) external onlyOwner {
        require(buyFee < 1000, "wZeny::setBuyFee: Buy fee exceeds max buy fee");
        _taxRates.buyFee = buyFee;
        emit SetTaxRate(_taxRates);
    }

    function setSellFee(
        uint256 sellFee
    ) external onlyOwner {
        require(sellFee < 1000, "wZeny::setSellFee: Sell fee exceeds max transfer fee");
        _taxRates.sellFee = sellFee;
        emit SetTaxRate(_taxRates);
    }

    function getSellFee() external view returns (uint256) {
        return _taxRates.sellFee;
    }

    function getBuyFee() external view returns (uint256) {
        return _taxRates.buyFee;
    }

    /* --------- transfer --------- */

    function _transfer(
        address sender,
        address recipient,
        uint256 amount
    ) internal {
        require(sender != address(0), "wZeny::_transfer: transfer from the zero address");
        require(recipient != address(0), "wZeny::_transfer: transfer to the zero address");
        require(amount > 0, "wZeny::_transfer: amount cannot be 0");

        _balances[sender] = _balances[sender].sub(amount, "wZeny::_transfer: transfer amount exceeds balance");
        uint256 recieveAmount = amount;

        uint256 checkFee;
        if (sender == DMMV2Pair) {
            // buy fee
            checkFee = _taxRates.buyFee;
        } else if (recipient == DMMV2Pair) {
            // sell fee
            require(_sellLimit >= amount, "wZeny::_transfer: sell amount exceeds sellLimit amount");
            checkFee = _taxRates.sellFee;
        } else {
            // transfer fee
            checkFee = 0;
        }

        uint256 feeAmount = amount.mul(checkFee).div(1000);
        if (feeAmount > 0) {
            swapTokensForEth(feeAmount, taxWallet1);
            swapTokensForEth(feeAmount, taxWallet2);
        }
        recieveAmount -= feeAmount;

        _balances[recipient] = _balances[recipient].add(recieveAmount);
        emit Transfer(sender, recipient, recieveAmount);
    }

    function swapTokensForEth(uint256 tokenAmount, address taxWallet) internal {
        address[] memory path = new address[](2);
        IERC20[] memory path1 = new IERC20[](2);
        path[0] = address(this);
        path[1] = address(DMMRouter02.weth());
        path1[0] = IERC20(address(this));
        path1[1] = IERC20(DMMRouter02.weth());

        _approve(address(this), address(DMMRouter02), tokenAmount);

        DMMRouter02.swapExactTokensForETH(
            tokenAmount,
            0,
            path,
            path1,
            taxWallet,
            block.timestamp
        );
    }

    receive() external payable {}
}
